{
	"name": "veracode-sca-ts",
	"displayName": "Veracode SCA (TS)",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:veracode-sca-ts.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "veracode-sca",
					"title": "Veracode SCA",
					"icon": "resources/left-arrow.svg"
				}
			]
		},
		"views": {
			"veracode-sca": [
				{
					"id": "repository-branchs",
					"name": "Branches"
				},
				{
					"id": "repository-tags",
					"name": "Tags"
				}
			]
		},
		"commands": [
			{
				"command": "veracode-sca-ts.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
